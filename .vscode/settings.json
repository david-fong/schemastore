{
  "files.associations": {
    "*.json": "json", // never autodetect as "JSON with comments", which allows trailing spaces.
    "*.ashx": "csharp",
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/*.{dll,DLL}": true,
    "**/*.{dll,DLL}.refresh": true,
  },
}
